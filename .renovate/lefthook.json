{
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "evilmartians/lefthook",
      "fileMatch": [
        "^.?lefthook.yml$"
      ],
      "matchStrings": [
        "min_version: (?<currentValue>1\\.5\\.1)\\n"
      ]
    }
  ]
}

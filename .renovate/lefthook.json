{
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "evilmartians/lefthook",
      "fileMatch": [
        "^.?lefthook.yml$"
      ],
      "matchStrings": [
        "min_version: (?<currentValue>[\\d\\.]+)"
      ],
      "autoReplaceStringTemplate": "min_version: {{{newValue}}}"
    }
  ]
}
